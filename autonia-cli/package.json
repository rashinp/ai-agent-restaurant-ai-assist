{
  "name": "@autonia/cli",
  "version": "0.1.4",
  "description": "CLI tool for deploying and managing Autonia applications",
  "type": "module",
  "bin": {
    "autonia": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc && chmod +x dist/index.js",
    "dev": "tsx src/index.ts",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "autonia",
    "cli",
    "deployment",
    "cloud-run"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^12.1.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.1",
    "prompts": "^2.4.2",
    "node-fetch": "^3.3.2",
    "form-data": "^4.0.1",
    "archiver": "^7.0.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.10",
    "@types/prompts": "^2.4.9",
    "@types/archiver": "^6.0.2",
    "typescript": "^5.7.3",
    "tsx": "^4.19.2"
  },
  "engines": {
    "node": ">=18"
  }
}
