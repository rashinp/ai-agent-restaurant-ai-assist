# This file specifies files that are intentionally untracked by gcloud.
# It follows the same syntax as .gitignore, with the addition of "#!include" directives.

# Include .gitignore rules
#!include:.gitignore

# Node.js
node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Environment files
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# Development files
.vscode/
.idea/
*.swp
*.swo
*~

# OS generated files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# Logs
logs/
*.log

# Runtime data
pids/
*.pid
*.seed
*.pid.lock

# Coverage directory used by tools like istanbul
coverage/
.nyc_output/

# TypeScript cache
*.tsbuildinfo

# Optional npm cache directory
.npm

# Optional REPL history
.node_repl_history

# Output of 'npm pack'
*.tgz

# Yarn Integrity file
.yarn-integrity

# dotenv environment variables file
.env.test

# parcel-bundler cache (https://parceljs.org/)
.cache
.parcel-cache

# next.js build output
.next

# nuxt.js build output
.nuxt

# vuepress build output
.vuepress/dist

# Serverless directories
.serverless

# FuseBox cache
.fusebox/

# DynamoDB Local files
.dynamodb/

# Development and testing files
test/
tests/
__tests__/
*.test.js
*.test.ts
*.spec.js
*.spec.ts

# Documentation
docs/
*.md
README*

# But keep dist/ folder for deployment
!dist/

# Git
.git/
.gitignore

# Build tools
webpack.config.js
rollup.config.js
jest.config.js
babel.config.js

# Linting
.eslintrc*
.prettierrc*

# TypeScript config (keep tsconfig.json for build)
# tsconfig.json

# Source maps (optional - uncomment if you don't want them in production)
# *.map

# Temporary files
tmp/
temp/
